<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item >
    <shape android:shape="oval">
      <solid android:color="@color/colorWhite"/>
      <stroke android:color="@color/shadow_outline" android:width="2dp"/>
    </shape>
  </item>

  <item
      android:bottom="1dp"
      android:left="1dp"
      android:right="1dp"
      android:top="1dp">
    <shape android:shape="oval">
      <stroke
          android:color="@color/shadow_light"
          android:width="2dp"/>
    </shape>
  </item>
</layer-list>