<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="KiviToolbarView">
    <attr name="center_logo" format="reference"/>
    <attr name="back_button" format="reference"/>
    <attr name="additional_menu_button" format="reference"/>
    <attr name="color_background" format="color"/>
  </declare-styleable>

  <declare-styleable name="RockersButtonView">
    <attr name="rb_textDescription" format="string|reference"/>
    <attr name="rb_top_image" format="reference"/>
    <attr name="rb_bottom_image" format="reference"/>
  </declare-styleable>

  <declare-styleable name="KiviDPadView">
    <attr name="arrowSize" format="dimension"/>
  </declare-styleable>


  <declare-styleable name="TextSeekBarView">
    <attr name="text" format="string" />
    <attr name="progress" format="integer" />
  </declare-styleable>

  <declare-styleable name="HorizontalSwitchView">
    <attr name="name" format="string" />
    <attr name="variant" format="string" />
  </declare-styleable>


  <declare-styleable name="HorizontalCardsView">
    <attr name="type" format="string" />
  </declare-styleable>

  <declare-styleable name="ButtonCustomView">
    <attr name="central_dr" format="reference"/>
  </declare-styleable>


  <declare-styleable name="AspectHeaderView">
    <attr name="header" format="string" />
    <attr name="row" format="string" />
  </declare-styleable>


</resources>